{
    "name": "@gsk/icons",
    "version": "0.0.1",
    "description": "Official React library for Untitled UI Icons",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "optimize:svg": "svgo -f icons/ -r",
        "build": "npm run build:icons && npm run build:tsc",
        "build:tsc": "tsup --minify",
        "build:icons": "npm run build:icons:main && npm run build:icons:misc && npm run build:icons:logos",
        "build:icons:main": "svgr -- icons",
        "build:icons:misc": "svgr --out-dir src/misc misc",
        "build:icons:logos": "svgr --out-dir src/logos logos",
        "release": "standard-version --release-as patch && git push --follow-tags && npm publish"
    },
    "files": [
        "README.md",
        "LICENSE",
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.mjs",
            "require": "./dist/*.js"
        }
    },
    "keywords": [
        "untitledui",
        "icons",
        "svg",
        "react"
    ],
    "author": "Dilshod Turobov",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@svgr/babel-plugin-remove-jsx-attribute": "^6.5.0",
        "@svgr/cli": "^6.5.1",
        "@types/react": "^18.3.3",
        "prettier": "^2.8.4",
        "react": "^18.3.1",
        "standard-version": "^9.5.0",
        "tsup": "^8.2.4",
        "typescript": "^5.5.4"
    },
    "peerDependencies": {
        "react": ">= 16"
    }
}
